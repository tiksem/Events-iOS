{
  "baseUrl": "http://azazai.com/api/",
  "requests": [
    {
      "method": "getEventsList",
      "limit": 10,
      "return": "LazyList<Event>",
      "key": "events",
      "mergeArgs": [
        {
          "name": "timeOut",
          "value": "true"
        }
      ]
    },
    {
      "method": "getUserEvents",
      "limit": 10,
      "return": "LazyList<Event>",
      "key": "Events",
      "args": [
        {
          "name": "mod",
          "type": "EventMode"
        }
      ],
      "mergeArgs": [
        {
          "name": "timeOut",
          "value": "true"
        }
      ]
    },
    {
      "method": "getTopComments",
      "url": "getCommentsList?offset=0",
      "args": [
        {
          "name": "id",
          "argName": "eventId",
          "type": "Int"
        },
        {
          "name": "limit",
          "argName": "maxCount",
          "type": "Int"
        }
      ],
      "return": "[Comment]",
      "key": "Comments"
    },
    {
      "method": "getCommentsList",
      "limit": 10,
      "return": "LazyList<Comment>",
      "key": "Comments",
      "args": [
        {
          "name": "id",
          "argName": "eventId",
          "type": "Int"
        }
      ]
    }
  ]
}